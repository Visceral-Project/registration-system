<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <navigation-rule>
        <display-name>Go To Login</display-name>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>loginPage</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Go To Admin Login</display-name>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>adminLoginPage</from-outcome>
            <to-view-id>/admin/Login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Go To Leaderboard</display-name>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>leaderboard</from-outcome>
            <to-view-id>/Leaderboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Registration.xhtml</display-name>
        <from-view-id>/Registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/RegConfirmation.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Registration.xhtml</display-name>
        <from-view-id>/Registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/Registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Login.xhtml</display-name>
        <from-view-id>/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>not_registered</from-outcome>
            <to-view-id>/Registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Login.xhtml</display-name>
        <from-view-id>/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>forgot_password</from-outcome>
            <to-view-id>/ForgotPassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ForgotPassword.xhtml</display-name>
        <from-view-id>/ForgotPassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>forgot_mail_sent</from-outcome>
            <to-view-id>/PasswordSent.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ForgotPassword.xhtml</display-name>
        <from-view-id>/ForgotPassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>forgot_mail_failed</from-outcome>
            <to-view-id>/ForgotPassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ResetPassword.xhtml</display-name>
        <from-view-id>/ResetPassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>passwordReseted</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>ResetPassword.xhtml</display-name>
        <from-view-id>/ResetPassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>passwordResetFailed</from-outcome>
            <to-view-id>/ResetPassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>AdminRegistration.xhtml</display-name>
        <from-view-id>/admin/Registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/RegConfirmation.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>AdminRegistration.xhtml</display-name>
        <from-view-id>/admin/Registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/admin/Registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Activate.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>activationPage</from-outcome>
            <to-view-id>/Activate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>AdminDashboard.xhtml</display-name>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>goToAdminDashboard</from-outcome>
            <to-view-id>/AdminDashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>Dashboard.xhtml</display-name>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>goToDashboard</from-outcome>
            <to-view-id>/Dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>VmCloneDialog.xhtml</display-name>
        <from-view-id>/Dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>askAboutCloneVm</from-outcome>
            <to-view-id>/VmCloneDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>BenchmarkManager.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>benchmarkManager</from-outcome>
            <to-view-id>/BenchmarkManager.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>FileManager.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fileManager</from-outcome>
            <to-view-id>/FileManager.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>BenchmarkFiles.xhtml</display-name>
        <from-view-id>/Dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>benchmarkFiles</from-outcome>
            <to-view-id>/BenchmarkFiles.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>AssignVM.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>assignVm</from-outcome>
            <to-view-id>/AssignVM.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>VmManager.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vmManager</from-outcome>
            <to-view-id>/VmManager.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule><navigation-rule>
        <display-name>AdminLeaderboard.xhtml</display-name>
        <from-view-id>/AdminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>adminLeaderboard</from-outcome>
            <to-view-id>/AdminLeaderboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule><navigation-rule>
        <display-name>Testpage.xhtml</display-name>
        <from-view-id>/Testpage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Testpage</from-outcome>
            <to-view-id>/Testpage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule><navigation-rule>
        <display-name>Results.xhtml</display-name>
        <from-view-id>/Dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>results</from-outcome>
            <to-view-id>/Results.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <validator>
        <display-name>Password Validator</display-name>
        <validator-id>passwordValidator</validator-id>
        <validator-class>eu.visceral.registration.validator.PasswordValidator</validator-class>
    </validator>
    <lifecycle>
        <phase-listener>eu.visceral.registration.AuthorizationListener</phase-listener>
    </lifecycle>
</faces-config>
